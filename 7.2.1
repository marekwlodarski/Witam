};

float Funkcja(struct Trojkat x)
{
    return x.a+x.b+x.c;
}

void Funkcja2(struct Trojkat troj1, struct Trojkat *troj2)
{
    (*troj2).a=troj1.a;
    (*troj2).b=troj1.b;
    (*troj2).c=troj1.c;
}

